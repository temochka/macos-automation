				<key>applescript</key>
				<string>property DAILY_NOTES_FOLDER : "üóì Daily Notes"

on alfred_script(q)
tell application "Notes"	set currentDate to do shell script "date +%F"	set noteTitle to currentDate &amp; " - Daily Note"	try		set theNote to first note in folder DAILY_NOTES_FOLDER whose name = noteTitle	on error		set noteBody to "&lt;html&gt;&lt;body&gt;&lt;div&gt;&lt;h1&gt;" &amp; noteTitle &amp; "&lt;/h1&gt;&lt;ul&gt;&lt;li&gt;&amp;nbsp;&lt;/li&gt;&lt;/ul&gt;&lt;/div&gt;&lt;/body&gt;&lt;/html&gt;"		set theNote to make new note in folder DAILY_NOTES_FOLDER in default account with properties {body:noteBody}	end try	show theNoteend tell
end alfred_script</string>
